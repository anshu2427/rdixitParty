  <main role="main" class="col-sm-9 ml-sm-auto col-md-10 pt-3">
          <h2><u>Show All PSP Executives</u></h2>
 <section class="get">
<table class="table table-bordered table-dark">
  <thead>
    <tr>
      <th scope="col">Executive Name</th>
      <th scope="col">Executive Profile</th>
      <th scope="col">Executive Photo</th>
      <th scope="col">Executive Brief Information</th>
      <th scope="col">Delete</th>
    </tr>
  </thead>
  <tbody>
       {{# each executiveModel }}

    <tr>
      <td>{{this.execName}}</td>
            <td>{{this.execPost}}</td>
      {{#if execPhoto}}
      <td> <img class="first-slide" src="../{{this.execPhoto}}" alt="First slide"   width="100" height="51"></td>
      {{else}}
      <td></td>
        {{/if}}
      <td>{{this.execDescription}}</td>
      <td><a class="btn btn-danger delete-executive" role="button" href='#' data-id={{this._id}} >DELETE</a></td>
    </tr>
      {{/each}}

  </tbody>

</table>  
</section>

